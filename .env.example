# Workspace Configuration
WORKSPACE_ID=your-workspace-id
WORKSPACE_NAME=your-workspace-name

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/workspace
# Or for SQLite: DATABASE_URL=file:./data/workspace.db

# OpenClaw Connection
OPENCLAW_GATEWAY_URL=http://openclaw:3000
OPENCLAW_WS_URL=ws://openclaw:3000/ws

# OpenRouter API Key (User's key)
OPENROUTER_API_KEY=sk-or-xxxx

# Auth
JWT_SECRET=your-super-secret-jwt-key
JWT_EXPIRES_IN=7d

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:4000
NEXT_PUBLIC_WS_URL=ws://localhost:4001

# Environment
NODE_ENV=development
